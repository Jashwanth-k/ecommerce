{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "Ecommerce application",
  "main": "server.js",
  "scripts": {
    "test": "jest && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "start": "node server.js",
    "reset:db": "npx sequelize-cli db:drop && npx sequelize-cli db:create",
    "pretest": "cross-env NODE_ENV=test && npm run reset:db"
  },
  "author": "Jashwanth",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^29.2.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "jest": "^29.2.0",
    "jest-extended": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.5"
  },
  "devDependencies": {
    "supertest": "^6.3.2"
  }
}
